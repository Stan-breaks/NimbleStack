package components

templ TechStackItem(name string, image string, description string) {
<div class="p-4 bg-white rounded-lg shadow text-center">
  <img src={ image } alt={ name } class="w-16 h-16 mx-auto" />
  <h4 class="mt-3 text-xl font-medium text-gray-800">{ name }</h4>
  <p class="mt-2 text-gray-600 text-sm">{ description }</p>
</div>
}
